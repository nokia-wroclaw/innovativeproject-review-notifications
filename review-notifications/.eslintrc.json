{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": ["react", "prettier"],
  "rules": {
    "no-console": "off",
    "quotes": ["error", "single"],
    "eol-last": ["error", "always"],
    "prettier/prettier": "error"
  }
}
